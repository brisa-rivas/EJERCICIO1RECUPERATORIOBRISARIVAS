Algoritmo Ejercicio12
	Definir distancia Como Real;
	Definir velocidad Como real;
	Definir tiempo como entero;
	
	//Ingreso de datos
	Escribir Sin Saltar "Ingrese la velocidad en m/s:";
	Leer velocidad;
	Escribir sin Saltar "Ingrese el tiempo en segundos:";
	Leer tiempo;
	
	//Proceso 
	distancia=velocidad*tiempo;
	//Informar 
	Escribir "La distancia alcanzada por un móvil a velocida ", velocidad, " en ", tiempo, " seg es de ", distancia;
	
FinAlgoritmo
