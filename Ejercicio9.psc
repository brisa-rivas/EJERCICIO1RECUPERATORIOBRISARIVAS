Algoritmo Ejercicio9
	Definir numeroN Como Entero;
	Definir sumaNros Como Entero;
	
	//Ingreso de datos
	Escribir Sin Saltar "Ingrese el valor del número hasta el cual quiere sumar:";
	Leer numeroN;
	
	//Proceso 
	sumaNros=(numeroN*(numeroN+1))/2;
	
	//Muestro la info
	Escribir "La suma de los primeros ",numeroN, " números enteros es ",sumaNros;
	
FinAlgoritmo
